<p id="notice"><%= notice %></p>

<h1>Listing Vote Methods</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Code</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vote_methods.each do |vote_method| %>
      <tr>
        <td><%= vote_method.name %></td>
        <td><%= vote_method.code %></td>
        <td><%= link_to 'Show', vote_method %></td>
        <td><%= link_to 'Edit', edit_vote_method_path(vote_method) %></td>
        <td><%= link_to 'Destroy', vote_method, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vote method', new_vote_method_path %>
